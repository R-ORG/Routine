{
  "expo": {
    "name": "RoutinesProject",
    "slug": "RoutinesProject",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/R_adaptive.png",
    "splash": {
      "image": "./assets/R_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
